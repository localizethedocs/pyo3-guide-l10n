msgid "\"house.py\""
msgstr ""

#: src/python-from-rust/calling-existing-code.md:349
msgid "\"house\""
msgstr ""

#: src/python-from-rust/calling-existing-code.md:353
msgid "\"House\""
msgstr ""

#: src/python-from-rust/calling-existing-code.md:354
