msgstr ""

#: src/get_started.md:32
msgid "PyO3 can be used to generate a native Python module."
msgstr ""

#: src/get_started.md:34
msgid "**`Cargo.toml`**"
msgstr ""

#: src/get_started.md:36
msgid ""
"```toml\n"
"[package]\n"
"name = \"string-sum\"\n"
"version = \"0.1.0\"\n"
"edition = \"2018\"\n"
"\n"
"[lib]\n"
"name = \"string_sum\"\n"
"crate-type = [\"cdylib\"]\n"
"\n"
"[dependencies.pyo3]\n"
"version = \"0.9.2\"\n"
"features = [\"extension-module\"]\n"
"```"
msgstr ""

#: src/get_started.md:51
msgid "**`src/lib.rs`**"
msgstr ""

