#: src/python_from_rust.md:415
msgid "Need to use a context manager from Rust?"
msgstr ""

#: src/python_from_rust.md:417
msgid "Use context managers by directly invoking `__enter__` and `__exit__`."
msgstr ""

#: src/python_from_rust.md:425
msgid ""
"r#\"\n"
"class House(object):\n"
"    def __init__(self, address):\n"
"        self.address = address\n"
"    def __enter__(self):\n"
"        print(f\"Welcome to {self.address}!\")\n"
"    def __exit__(self, type, value, traceback):\n"
"        if type:\n"
"            print(f\"Sorry you had {type} trouble at {self.address}\")\n"
"        else:\n"
"            print(f\"Thank you for visiting {self.address}, come again soon!"
"\")\n"
"\n"
"        \"#"
msgstr ""

#: src/python_from_rust.md:437
msgid "\"house.py\""
msgstr ""

#: src/python_from_rust.md:437
msgid "\"house\""
msgstr ""

#: src/python_from_rust.md:439
msgid "\"House\""
msgstr ""

#: src/python_from_rust.md:440
msgid "\"123 Main Street\""
msgstr ""

#: src/python_from_rust.md:442
msgid "\"__enter__\""
msgstr ""

#: src/python_from_rust.md:444
msgid "\"undefined_variable + 1\""
msgstr ""

